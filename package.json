{
  "name": "react-express-ssr-boilerplate",
  "version": "1.0.0",
  "description": "A starter for a project using ExpressJs and ReactJs with server side rendering.",
  "scripts": {
    "start": "cross-env NODE_ENV=development TARGET=client nodemon --require dotenv/config",
    "server": "cross-env NODE_ENV=production node -r dotenv/config dist/index.js",
    "analyze": "source-map-explorer dist/bundle.*",
    "build:client": "cross-env NODE_ENV=production TARGET=client webpack",
    "build:server": "cross-env NODE_ENV=production TARGET=server webpack",
    "build": "run-s build:client build:server",
    "prebuild": "rimraf dist",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "precommit": "pretty-quick --staged"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/weyheyhey/react-express-ssr-boilerplate.git"
  },
  "keywords": [
    "react",
    "express",
    "webpack",
    "styled",
    "redux",
    "ssr"
  ],
  "author": "weyheyhey",
  "license": "Unlicense",
  "dependencies": {
    "asset-require-hook": "^1.2.0",
    "compression": "^1.7.2",
    "connected-react-router": "^4.4.1",
    "dotenv": "^6.0.0",
    "express": "^4.16.3",
    "helmet": "^3.13.0",
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "lru-cache": "^4.1.3",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-helmet": "^5.2.0",
    "react-hot-loader": "^4.3.5",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-config": "^1.0.0-beta.4",
    "redux": "^4.0.0",
    "redux-actions": "^2.6.1",
    "redux-devtools-extension": "2.13.5",
    "redux-thunk": "2.3.0",
    "reselect": "^3.0.1",
    "serialize-javascript": "^1.5.0",
    "styled-components": "^3.4.5",
    "styled-normalize": "^8.0.0",
    "webpack-dev-middleware": "^3.2.0",
    "webpack-hot-middleware": "^2.22.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.4",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.4",
    "@babel/plugin-transform-react-inline-elements": "^7.0.0-rc.4",
    "@babel/preset-env": "^7.0.0-rc.4",
    "@babel/preset-react": "^7.0.0-rc.4",
    "@babel/register": "^7.0.0-rc.4",
    "@commitlint/cli": "^7.0.0",
    "@commitlint/config-conventional": "^7.0.1",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^8.0.0-beta",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-styled-components": "^1.5.1",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "eslint": "^5.4.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-import-resolver-babel-module": "5.0.0-beta.1",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7.11.1",
